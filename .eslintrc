{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": 0,
    "global-require": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    'no-extra-boolean-cast': 0,
    "new-cap": 0,
    "max-len": ["error", 160],
    "no-confusing-arrow": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0, 
  }
}
